<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>xyz.joestr</groupId>
  <artifactId>zonemenu_dev</artifactId>
  <version>build_8</version>
  <name>ZoneMenu_Dev</name>
  <properties>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
  </properties>
  <repositories>
    <repository>
      <id>spigot-repo</id>
      <url>https://hub.spigotmc.org/nexus/content/repositories/snapshots/</url>
    </repository>
    <repository>
        <id>sk89q-repo</id>
        <url>http://repo.joestr.xyz/zonemenu/</url>
    </repository>
  </repositories>
  <dependencies>
    <!--Spigot API-->
    <dependency>
      <groupId>org.spigotmc</groupId>
      <artifactId>spigot-api</artifactId>
      <version>1.12.1-R0.1-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>
    <!--Bukkit API-->
    <!--<dependency>
      <groupId>org.bukkit</groupId>
      <artifactId>bukkit</artifactId>
      <version>1.12.1-R0.1-SNAPSHOT</version>
      <scope>provided</scope>
	</dependency>-->
    <dependency>
    	<groupId>com.sk89q</groupId>
    	<artifactId>worldguard</artifactId>
    	<version>6.2.1</version>
    	<scope>provided</scope>
    </dependency>
    <dependency>
    	<groupId>com.sk89q</groupId>
    	<artifactId>worldedit-bukkit</artifactId>
    	<version>6.1.7.3</version>
    	<scope>provided</scope>
    </dependency>
  </dependencies>
  <description>Simple, interactive menu for region creation with WorldGuard (and WorldEdit) for players. (Spigot)</description>
  <url>https://repo.joestr.xyz/zonemenu</url>
  <build>
    <resources>
      <resource>
        <directory>
          src/main/java
        </directory>
        <filtering>true</filtering>
      </resource>
      <resource>
        <directory>
          src/main/resources
        </directory>
        <filtering>true</filtering>
      </resource>
    </resources>
  </build>
</project>