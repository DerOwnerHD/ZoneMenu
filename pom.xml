<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>xyz.joestr</groupId>
  <artifactId>zonemenu-dev</artifactId>
  <version>0.1.0</version>
  <name>ZoneMenu Development</name>
  <properties>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
  </properties>
  <repositories>
    <repository>
      <id>joestr-releases</id>
      <url>https://repo2.joestr.xyz/repositoy/releases/</url>
    </repository>
    <repository>
      <id>joestr-snapshots</id>
      <url>https://repo2.joestr.xyz/repository/snapshots/</url>
    </repository>
    <!--<repository>
    <id>sk89q-repo</id>
    <url>http://maven.sk89q.com/artifactory/repo/</url>
    </repository>-->
  </repositories>
  <dependencies>
    <!--Spigot API-->
    <dependency>
        <groupId>org.spigotmc</groupId>
        <artifactId>spigot-api</artifactId>
        <version>1.13.2-R0.1-SNAPSHOT</version>
        <scope>provided</scope>
    </dependency>
    <!--Bukkit API-->
    <!--<dependency>
      <groupId>org.bukkit</groupId>
      <artifactId>bukkit</artifactId>
      <version>1.13.2-R0.1-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>-->
    <dependency>
    	<groupId>com.sk89q.worldedit</groupId>
    	<artifactId>worldedit-bukkit</artifactId>
    	<version>7.0.0-SNAPSHOT</version>
    	<scope>provided</scope>
    </dependency>
    <dependency>
    	<groupId>com.sk89q.worldguard</groupId>
    	<artifactId>worldguard-legacy</artifactId>
    	<version>7.0.0-SNAPSHOT</version>
    	<scope>provided</scope>
        <exclusions>
            <exclusion>
                <groupId>com.sk89q</groupId>
                <artifactId>commandbook</artifactId>
            </exclusion>
        </exclusions>
    </dependency>
  </dependencies>
  <description>Simple, interactive menu for region creation with WorldGuard (and WorldEdit) for players. (Spigot)</description>
  <url>https://repo.joestr.xyz/zonemenu</url>
  <build>
    <resources>
      <resource>
        <directory>src/main</directory>
        <filtering>true</filtering>
      </resource>
    </resources>
  </build>
  <organization>
  	<name>joestr</name>
  	<url>https://joestr.xyz</url>
  </organization>
  <scm>
  	<url>https://github.com/joestr/ZoneMenu/tree/dev</url>
  	<developerConnection>joestr</developerConnection>
  	<tag>zone, worldedit, worldguard, spigot-plugin</tag>
  </scm>
  <ciManagement>
  	<url>https://ci.joestr.xyz/job/ZoneMenu_Dev/</url>
  </ciManagement>
  <issueManagement>
  	<url>https://github.com/joestr/ZoneMenu/issues</url>
  </issueManagement>
</project>
